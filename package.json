{
  "name": "activation-scripts",
  "type": "module",
  "version": "1.5.0",
  "packageManager": "pnpm@8.15.1",
  "description": "",
  "author": "wibus-wee <wibus@qq.com>",
  "keywords": [],
  "scripts": {
    "build:core": "pnpm -C packages/core run build && cp dist/activator.js activator.js",
    "build:dashboard": "pnpm -C packages/dashboard run build",
    "build:generator": "pnpm -C packages/generator run build",
    "build": "npm run build:core && npm run build:generator",
    "dev:dashboard": "pnpm -C packages/dashboard run dev",
    "start:generator": "pnpm -C packages/generator run start",
    "typecheck": "tsc --noEmit && pnpm -C packages/dashboard run typecheck",
    "prepare": "simple-git-hooks",
    "lint": "eslint ."
  },
  "dependencies": {
    "@antfu/eslint-config": "^2.6.4",
    "commander": "^12.0.0",
    "eslint": "^8.56.0",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-terser": "^0.4.3",
    "@types/node": "^20.6.0",
    "esno": "^0.17.0",
    "lint-staged": "^15.2.0",
    "rollup": "^3.29.1",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-ts": "^3.4.5",
    "simple-git-hooks": "^2.9.0",
    "toml": "^3.0.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
